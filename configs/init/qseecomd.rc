# vim: set ft=sh:

service vendor.qseecomd /vendor/bin/qseecomd
    class core
    user root
    group root
    disabled
    writepid /dev/cpuset/system-background/tasks

on late-init
    start vendor.qseecomd

on post-fs-data
    # Create directory for TZ Apps
    mkdir /data/misc/qsee 0770 system system

